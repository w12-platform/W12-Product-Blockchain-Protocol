{
  "name": "w12-product-contracts",
  "version": "0.23.2",
  "description": "W12 is an open protocol for the creation of DAOs. The protocol consists of smart contract templates (no programming skills required), DAO governance and a decentralized oracles network that controls execution of the project roadmap and milestones.",
  "keywords": [
    "w12",
    "ethereum",
    "smartcontract",
    "token",
    "crowdsale",
    "ico",
    "dao",
    "decentralized"
  ],
  "homepage": "https://github.com/w12-platform/W12-Product-Blockchain-Protocol.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/w12-platform/W12-Product-Blockchain-Protocol.git"
  },
  "author": "W12.io",
  "scripts": {
    "version": "echo \"$npm_package_version\"",
    "estimate-gas": "node ./shared/scripts/estimate-gas.js",
    "build": "npm run clean && npm run t:compile && npm run abi",
    "clean": "rm -frd ./build",
    "abi": "node ./shared/scripts/generate-abi.js",
    "dev-net": "ganache-cli -p 7545 -g 1 -l 8000000 &",
    "ci-test": "cp example.config.js config.js && npm run dev-net > /dev/null && npm run test",
    "ci-deploy-test-net": "chmod +x ./shared/scripts/deploy.sh && ./shared/scripts/deploy.sh",
    "t:compile": "truffle compile",
    "t:migrate": "truffle migrate",
    "t:migrate:test": "truffle migrate --network test",
    "t:migrate:r:test": "truffle migrate --reset --network test",
    "t:migrate:r": "truffle migrate --reset",
    "t:test": "truffle test",
    "flat": "truffle-flattener",
    "test": "npm run t:test"
  },
  "devDependencies": {
    "@redtea/semint": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.23.0",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-testrpc": "^6.0.1",
    "ethereumjs-util": "^5.2.0",
    "ethjs-abi": "^0.2.1",
    "ganache-cli": "^6.1.8",
    "mocha-lcov-reporter": "^1.3.0",
    "semver": "^5.5.1",
    "solc": "^0.4.24",
    "solidity-coverage": "^0.5.5",
    "solium": "^1.1.8",
    "truffle": "^4.1.13",
    "truffle-hdwallet-provider": "0.0.7-beta.1",
    "utf8-bytes": "0.0.1"
  },
  "dependencies": {
    "chai-arrays": "^2.0.0",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.11",
    "nanoid": "^1.1.0",
    "openzeppelin-solidity": "1.12.0",
    "sha3": "^1.2.2",
    "solidity-bytes-utils": "0.0.5",
    "truffle-flattener": "^1.2.8"
  }
}
